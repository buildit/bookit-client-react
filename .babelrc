{
  "presets": [ [ "env", { "modules": false } ], "react" ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-class-properties",
    [ "transform-runtime", { "polyfill": false, "regenerator": true } ],
    [ "transform-object-rest-spread", { "useBuiltIns": true } ]
  ],
  "env": {
    "test": {
      "presets": [ [ "env" ], "react" ]
    }
  }
}
